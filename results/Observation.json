{
    "model": "Observation",
    "storageType": "sql",
    "attributes": {
        "observation_primary_key": "Int",
        "collector": {
            "type": "String",
            "description": "The name or identifier of the entity which collected the observation"
        },
        "observationDbId": {
            "type": "String",
            "description": "The ID which uniquely identifies an observation"
        },
        "observationTimeStamp": {
            "type": "String",
            "description": "The date and time when this observation was made"
        },
        "uploadedBy": {
            "type": "String",
            "description": "The name or id of the user who uploaded the observation to the database system"
        },
        "value": {
            "type": "String",
            "description": "The value of the data collected as an observation"
        },
        "germplasm_id": "Int",
        "observation_unit_id": "Int",
        "observation_variable_id": "Int",
        "study_id": "Int",
        "images_ids": "[ Int ]"
    },
    "associations": {
        "germplasm": {
            "type": "many_to_one",
            "implementation": "foreignkey",
            "reverseAssociation": "observations",
            "target": "Germplasm",
            "targetKey": "observations_ids",
            "sourceKey": "germplasm_id",
            "keysIn": "Observation",
            "targetStorageType": "sql"
        },
        "observationUnit": {
            "type": "many_to_one",
            "implementation": "foreignkey",
            "reverseAssociation": "observations",
            "target": "ObservationUnit",
            "targetKey": "observations_ids",
            "sourceKey": "observation_unit_id",
            "keysIn": "Observation",
            "targetStorageType": "sql"
        },
        "observationVariable": {
            "type": "many_to_one",
            "implementation": "foreignkey",
            "reverseAssociation": "observations",
            "target": "ObservationVariable",
            "targetKey": "observations_ids",
            "sourceKey": "observation_variable_id",
            "keysIn": "Observation",
            "targetStorageType": "sql"
        },
        "study": {
            "type": "many_to_one",
            "implementation": "foreignkey",
            "reverseAssociation": "observations",
            "target": "Study",
            "targetKey": "observations_ids",
            "sourceKey": "study_id",
            "keysIn": "Observation",
            "targetStorageType": "sql"
        },
        "images": {
            "type": "many_to_many",
            "implementation": "foreignkey",
            "reverseAssociation": "observations",
            "target": "Image",
            "targetKey": "observations_ids",
            "sourceKey": "images_ids",
            "keysIn": "Observation",
            "targetStorageType": "sql"
        },
        "internalId": "observation_primary_key"
    }
}