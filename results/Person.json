        },
        "programs": {
            "type": "one_to_many",
            "implementation": "foreignkey",
            "reverseAssociation": "leadPerson",
            "target": "Program",
            "targetKey": "Program_id",
            "keysIn": "Person",
            "targetStorageType": "sql"
        }
    }
}